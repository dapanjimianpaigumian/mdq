<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sdjr.mdq.ui.YDYActivity">
    <android.support.v4.view.ViewPager
        android:id="@+id/ydy_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </android.support.v4.view.ViewPager>
    <LinearLayout
        android:id="@+id/ydy_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="40dp"
        android:gravity="center_horizontal">
    </LinearLayout>
    <ImageButton
        android:layout_width="115.5dp"
        android:layout_height="33.5dp"
        android:id="@+id/guide_ib_start"
        android:layout_centerHorizontal="true"
        android:layout_above="@+id/ydy_lin"
        android:background="@drawable/ydybtn"
        android:visibility="gone"/>
</RelativeLayout>
