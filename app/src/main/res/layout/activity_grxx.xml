<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#f7f7f7"
    tools:context="com.sdjr.mdq.ui.grxx.GRXXActivity">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:background="@color/home"
        >
        <ImageView
            android:id="@+id/grxx_img01"
            android:layout_width="100dp"
            android:layout_height="20dp"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="29dp"
            android:src="@drawable/fanhui"
            android:paddingRight="80dp"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="工作信息"
            android:textColor="#ffffff"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25dp"
            android:textSize="20dp"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#f7f7f7"
        />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="#f7f7f7"
            >
        <LinearLayout
            android:id="@+id/grxx_lin1"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:orientation="vertical"
            android:background="#ffffff"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:text="公司名称"
                    android:textSize="15dp"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/hei"
                    android:id="@+id/textView" />

                <EditText
                    android:id="@+id/login_et1"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:hint="请输入公司名称"
                    android:textSize="15dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@null"
                    />
                <ImageView
                    android:id="@+id/grxx_img1"
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xing"
                    android:layout_gravity="center"
                    android:layout_marginRight="32dp"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/hui"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/grxx_lin2"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:orientation="vertical"
            android:background="#ffffff"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:text="公司电话"
                    android:textSize="15dp"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/hei"
                    />
                <EditText
                    android:id="@+id/login_et2"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:hint="请输入公司电话"
                    android:textSize="15dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@null"
                    android:maxLength="11"
                    android:phoneNumber="true"
                    />
                <ImageView
                    android:id="@+id/grxx_img2"
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xing"
                    android:layout_gravity="center"
                    android:layout_marginRight="32dp"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/hui"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/grxx_lin3"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:orientation="vertical"
            android:background="#ffffff"
            >
            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:text="所在职务"
                    android:textSize="15dp"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/hei"
                    />
                <EditText
                    android:id="@+id/login_et3"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:hint="请输入所在职务"
                    android:textSize="15dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@null"
                    />
                <ImageView
                    android:id="@+id/grxx_img3"
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xing"
                    android:layout_gravity="center"
                    android:layout_marginRight="32dp"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/hui"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/grxx_lin4"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:orientation="vertical"
            android:background="#ffffff"
            >
            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:text="月收入"
                    android:textSize="15dp"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/hei"
                    />
                <EditText
                    android:id="@+id/login_et4"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:hint="请输入收入"
                    android:textSize="15dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@null"
                    />
                <ImageView
                    android:id="@+id/grxx_img4"
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xing"
                    android:layout_gravity="center"
                    android:layout_marginRight="32dp"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/hui"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/grxx_lin5"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:orientation="vertical"
            android:background="#ffffff"
            >
            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:text="所属行业"
                    android:textSize="15dp"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/hei"
                    />

                <EditText
                    android:id="@+id/login_et5"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:hint="请输入所属行业"
                    android:textSize="15dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@null"
                    />
                <ImageView
                    android:id="@+id/grxx_img5"
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xing"
                    android:layout_gravity="center"
                    android:layout_marginRight="32dp"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/hui"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/grxx_lin6"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:orientation="vertical"
            android:background="#ffffff"
            >
            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:text="公司邮箱"
                    android:textSize="15dp"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/hei"
                    />
                <EditText
                    android:id="@+id/login_et6"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:hint="请输入公司邮箱"
                    android:textSize="15dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@null"
                    />
                <ImageView
                    android:id="@+id/grxx_img6"
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xing"
                    android:layout_gravity="center"
                    android:layout_marginRight="32dp"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/hui"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/grxx_lin7"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:orientation="vertical"
            android:background="#ffffff"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:text="公司规模"
                    android:textSize="15dp"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:textColor="@color/hei"
                    />
                <EditText
                    android:id="@+id/login_et7"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:hint="请输入公司规模"
                    android:textSize="15dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="15dp"
                    android:background="@null"
                    />
                <ImageView
                    android:id="@+id/grxx_img7"
                    android:layout_width="18dp"
                    android:layout_height="20dp"
                    android:src="@drawable/xing"
                    android:layout_gravity="center"
                    android:layout_marginRight="32dp"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/hui"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                />
        </LinearLayout>
            <Button
                android:id="@+id/grxx_btn"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:background="@drawable/shape1"
                android:text="提交"
                android:textSize="15dp"
                android:textColor="@color/bai"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:layout_marginTop="35dp"
                android:layout_marginBottom="35dp"
                />
        </LinearLayout>
    </ScrollView>
</LinearLayout>
